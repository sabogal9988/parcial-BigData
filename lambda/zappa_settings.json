        "exclude": [
            "boto3",
            "dateutil",
            "botocore",
            "s3transfer",
            "concurrent"
        ],
        "project_name": "lambda",
        "runtime": "python3.9",
        "s3_bucket": "dolar-raw-sebastian-10347",
        "keep_warm": false,
        "apigateway_enabled": false,
        "manage_roles": false,
        "role_name": "LabRole",
        
        "events": [
      {
        "function": "app.handler",
        "expression": "cron(0 23 ? * * *)"
      }
    ]
    
